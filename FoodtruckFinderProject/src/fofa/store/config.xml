<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>

	<typeAliases>
		<typeAlias type="fofa.domain.Advertise" alias="Advertise" />
		<typeAlias type="fofa.domain.Favorite" alias="Favorite" />
		<typeAlias type="fofa.domain.Follow" alias="Follow" />
		<typeAlias type="fofa.domain.Foodtruck" alias="Foodtruck" />
		<typeAlias type="fofa.domain.Image" alias="Image" />
		<typeAlias type="fofa.domain.Member" alias="Member" />
		<typeAlias type="fofa.domain.Menu" alias="Menu" />
		<typeAlias type="fofa.domain.Recommand" alias="Recommand" />
		<typeAlias type="fofa.domain.Report" alias="Report" />
		<typeAlias type="fofa.domain.Review" alias="Review" />
		<typeAlias type="fofa.domain.Sales" alias="Sales" />
		<typeAlias type="fofa.domain.Survey" alias="Survey" />
		<typeAlias type="fofa.domain.SurveyItem" alias="SurveyItem" />
		<typeAlias type="fofa.domain.SurveyReply" alias="SurveyReply" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC"></transactionManager>
			<dataSource type="POOLED">
				<property name="driver" value="oracle.jdbc.driver.OracleDriver" />
				<property name="url" value="jdbc:oracle:thin:@localhost:1521:xe" />
				<property name="username" value="hr" />
				<property name="password" value="hr" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="store/mapper/AdvertiseMapper.xml"/>
		<mapper resource="store/mapper/FavoriteMapper.xml"/>
		<mapper resource="store/mapper/FollowMapper.xml"/>
		<mapper resource="store/mapper/FoodtruckMapper.xml"/>
		<mapper resource="store/mapper/ImageMapper.xml"/>
		<mapper resource="store/mapper/MemberMapper.xml"/>
		<mapper resource="store/mapper/MenuMapper.xml"/>
		<mapper resource="store/mapper/RecommandMapper.xml"/>
		<mapper resource="store/mapper/ReportMapper.xml"/>
		<mapper resource="store/mapper/ReviewMapper.xml"/>
		<mapper resource="store/mapper/SalesMapper.xml"/>
		<mapper resource="store/mapper/SellerMapper.xml"/>
		<mapper resource="store/mapper/SurveyItemMapper.xml"/>
		<mapper resource="store/mapper/SurveyReplyMapper.xml"/>
		<mapper resource="store/mapper/SurveyMapper.xml"/>
	</mappers>

</configuration>