<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>

	<typeAliases>
		<typeAlias type="domain.Advertise" alias="Advertise" />
		<typeAlias type="domain.Favorite" alias="Favorite" />
		<typeAlias type="domain.Follow" alias="Follow" />
		<typeAlias type="domain.Foodtruck" alias="Foodtruck" />
		<typeAlias type="domain.Image" alias="Image" />
		<typeAlias type="domain.Member" alias="Member" />
		<typeAlias type="domain.Menu" alias="Menu" />
		<typeAlias type="domain.Recommand" alias="Recommand" />
		<typeAlias type="domain.Report" alias="Report" />
		<typeAlias type="domain.Review" alias="Review" />
		<typeAlias type="domain.Sales" alias="Sales" />
		<typeAlias type="domain.Survey" alias="Survey" />
		<typeAlias type="domain.SurveyItem" alias="SurveyItem" />
		<typeAlias type="domain.SurveyReply" alias="SurveyReply" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC"></transactionManager>
			<dataSource type="POOLED">
				<property name="driver" value="oracle.jdbc.driver.OracleDriver" />
				<property name="url" value="jdbc:oracle:thin:@localhost:1521:xe" />
				<property name="username" value="hr" />
				<property name="password" value="hr" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="store/mapper/AdvertiseMapper.xml"/>
		<mapper resource="store/mapper/FavoriteMapper.xml"/>
		<mapper resource="store/mapper/FollowMapper.xml"/>
		<mapper resource="store/mapper/FoodtruckMapper.xml"/>
		<mapper resource="store/mapper/ImageMapper.xml"/>
		<mapper resource="store/mapper/MemberMapper.xml"/>
		<mapper resource="store/mapper/MenuMapper.xml"/>
		<mapper resource="store/mapper/Recommand.xml"/>
		<mapper resource="store/mapper/Report.xml"/>
		<mapper resource="store/mapper/Review.xml"/>
		<mapper resource="store/mapper/Sales.xml"/>
		<mapper resource="store/mapper/Seller.xml"/>
		<mapper resource="store/mapper/SurveyItem.xml"/>
		<mapper resource="store/mapper/SurveyReply.xml"/>
		<mapper resource="store/mapper/Survey.xml"/>
	</mappers>

</configuration>